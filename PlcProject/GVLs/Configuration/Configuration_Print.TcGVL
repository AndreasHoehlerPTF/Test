  (nPhInst := 16, nPcInst := 6, nPm := 2, nPg := 1, nPa := 1, nPs := 1, nPc := 2, nPcCh := 1),    // 16
  (nPhInst := 17, nPcInst := 6, nPm := 2, nPg := 1, nPa := 1, nPs := 1, nPc := 2, nPcCh := 2),    // 17
