[ 
  // PM 1 - NOTE: nPC global, nPcPc per pc, other values relative to PM
  (nPhInst := 1,  nPcInst := 1, nPm := 1, nPg := 1, nPa := 1, nPs := 1, nPc := 1, nPcCh := 1),    // 1
  (nPhInst := 2,  nPcInst := 1, nPm := 1, nPg := 1, nPa := 1, nPs := 1, nPc := 1, nPcCh := 2),    // 2
  (nPhInst := 3,  nPcInst := 1, nPm := 1, nPg := 1, nPa := 1, nPs := 1, nPc := 1, nPcCh := 3),    // 3
  (nPhInst := 4,  nPcInst := 2, nPm := 1, nPg := 1, nPa := 1, nPs := 1, nPc := 2, nPcCh := 1),    // 4
  (nPhInst := 5,  nPcInst := 2, nPm := 1, nPg := 1, nPa := 1, nPs := 1, nPc := 2, nPcCh := 2),    // 5
  (nPhInst := 6,  nPcInst := 2, nPm := 1, nPg := 1, nPa := 1, nPs := 1, nPc := 2, nPcCh := 3),    // 6
  (nPhInst := 7,  nPcInst := 3, nPm := 1, nPg := 2, nPa := 1, nPs := 1, nPc := 3, nPcCh := 1),    // 7
  (nPhInst := 8,  nPcInst := 3, nPm := 1, nPg := 2, nPa := 1, nPs := 1, nPc := 3, nPcCh := 2),    // 8
  (nPhInst := 9,  nPcInst := 3, nPm := 1, nPg := 2, nPa := 1, nPs := 1, nPc := 3, nPcCh := 3),    // 9
  (nPhInst := 10, nPcInst := 4, nPm := 1, nPg := 2, nPa := 1, nPs := 1, nPc := 4, nPcCh := 1),    // 10
  (nPhInst := 11, nPcInst := 4, nPm := 1, nPg := 2, nPa := 1, nPs := 1, nPc := 4, nPcCh := 2),    // 11
  (nPhInst := 12, nPcInst := 4, nPm := 1, nPg := 2, nPa := 1, nPs := 1, nPc := 4, nPcCh := 3),    // 12
  // PM 2 - NOTE: nPC global, nPcPc per pc, other values relative to PM
