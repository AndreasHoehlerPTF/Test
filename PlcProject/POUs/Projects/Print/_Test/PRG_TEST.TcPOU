(*************************************************************************************************)
(*  DESCRIPTION:                                                                   <description> *)
(*    Controls the test functions.                                                               *)
(*                                                                                </description> *)
(*************************************************************************************************)

VAR
	bL_FirstRun           : BOOL := TRUE;

  (*Test CONFIGURATION*)
  bLogRateKill : BOOL;
  tLog : TON;
  nLogCt : UDINT;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[(* INIT First action init call*)
IF bL_FirstRun THEN
	bL_FirstRun := FALSE;
	Init();
END_IF;

(*** configure tests ****************************)
(*--> un-comment the defines in GVL_TestCfg*)
(************************************************)

IF bLogRateKill THEN
  tLog(IN:=NOT tLog.Q, PT:=T#10MS);
  nLogCt := nLogCt + 1;
  F_LogWarning(F_Concat2('LogKill ct: ', TO_STRING(nLogCt)));
END_IF

(*** Library testing ***)
{IF defined (variable: GVL_TestCfg.do_LibTest)}
  LibTest();
{END_IF}

]]></ST>
    </Implementation>
    <Action Name="Init" Id="{15466f12-fe50-464a-bc06-a462dd113028}">
      <Implementation>
        <ST><![CDATA[
; // nothing to do]]></ST>
      </Implementation>
    </Action>
    <Action Name="LibTest" Id="{df43acd7-7245-4dbb-a1db-5e797fac5e48}">
      <Implementation>
        <ST><![CDATA[CalmarCommons.PRG_TEST();
CalmarCompPrint.PRG_TEST();]]></ST>
      </Implementation>
    </Action>
